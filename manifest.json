{
    "manifest_version": 3,
    "name": "YouTube English Only",
    "version": "4.0",
    "description": "Shows only English videos using local AI language detection",
    "permissions": ["activeTab"],
    "host_permissions": ["*://*.youtube.com/*"],
    "content_scripts": [{
        "matches": ["*://*.youtube.com/*"],
        "js": ["eld.M60.min.js", "content.js"],
        "run_at": "document_idle"
    }]
}